{
    "version": 2,
    "name": "My-Port-Folio",
    "alias": ["fititsolution.com","www.fititsolution.com"],
    "github": {
    "autoAlias": false
    },
    "builds": [
      {
        "src": "package.json",
        "use": "@now/static-build"
      }
    ],
    "routes": [
	{
      "src": "/(.*)",
      "status": 301,
      "headers": { "Location": "https://fititsolution.com/$1" }
    },
      { "src": "(.*\\..*)", "dest": "$1" },
      { "src": ".*", "dest": "/index.html" }
    ]
  }
