{
    "version": 2,
    "name": "My-Port-Folio",
    "alias": ["fititsolution.com","www.fititsolution.com"],
    "github": {
    "autoAlias": false
    },
    "builds": [
      {
        "src": "package.json",
        "use": "@now/static-build"
      }
    ],
    "routes": [
      { "src": "(.*\\..*)", "dest": "$1",
        //"headers": { "cache-control": "s-maxage=86400" },
        "headers": {"cache-control": "public, max-age=86400"}
       },
      { "src": ".*", "dest": "/index.html",
        //"headers": { "cache-control": "s-maxage=86400" }
        "headers": {"cache-control": "public, max-age=86400"}
        
       }

    ]
  }
